{
  "name": "dans-mustache",
  "displayName": "Dan's Mustache",
  "version": "1.1.7",
  "description": "Custom Mustache language profile",
  "publisher": "DanDun",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dan-OH/dans-mustache"
  },
  "contributes": {
    "languages": [
      {
        "id": "mustache",
        "aliases": [
          "Mustache (Dans)",
          "mustache"
        ],
        "extensions": [
          ".mustache",
          ".mst",
          ".mu",
          ".stache"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mustache",
        "scopeName": "source.mustache",
        "path": "./syntaxes/mustache.tmLanguage.json",
        "embeddedLanguages": {
          "text.html": "html",
          "source.css": "css",
          "source.js": "javascript",
          "source.python": "python",
          "source.smarty": "smarty"
        }
      }
    ],
    "snippets": [
      {
        "language": "mustache",
        "path": "./snippets/mustache.code-snippets"
      }
    ]
  },
  "author": "DanDun",
  "license": "ISC"
}
